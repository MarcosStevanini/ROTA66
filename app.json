{
  "expo": {
    "name": "Rota66",
    "slug": "Rota66",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "version": "1.0.1",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#065099"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#132440",
        "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "MEDIA_CONTENT_CONTROL", "AUDIOMIXER", "FOREGROUND_SERVICE", "WAKE_LOCK"]
      }
    },
    "web": {
      "favicon": "./src/assets/logo.png"
    }
  }
}
